{% for blocks in section.blocks %}
  section
{% endfor %}

{% schema %}
{
  "name": "CustomSection",
  "settings": [],
  "blocks": [
    {
      "type": "headline_with_paragraph",
      "name": "Headline with Paragraph",
      "settings": [
        {
          "type": "text",
          "label": "Headline",
          "id": "headline",
          "default": "Welcome to Our Store"
        },
        {
          "type": "richtext",
          "label": "Paragraph",
          "id": "paragraph",
          "default": "<p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>"
        }
      ]
    },
    {
      "type": "selected_products",
      "name": "List of Selected Products",
      "settings": [
        {
          "type": "product_picker",
          "label": "Select Products",
          "id": "selected_products",
          "info": "Choose the products you want to display in the list."
        }
      ]
    },
    {
      "type": "image_break",
      "name": "Image Break",
      "settings": [
        {
          "type": "image",
          "label": "Image",
          "id": "image",
          "default": {
            "alt": "Image Alt Text",
            "src": "https://example.com/image.jpg"
          }
        }
      ]
    }
  ]
}
{% endschema %}
